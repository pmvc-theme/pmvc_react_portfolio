{
  "version": "0.4.5",
  "name": "pmvc_react_portfolio",
  "repository": {
    "type": "git",
    "url": "https://github.com/pmvc-theme/pmvc_react_portfolio"
  },
  "description": "React portfolio",
  "keywords": [
    "react",
    "react-component"
  ],
  "author": "Hill <hill@kimo.com>",
  "license": "MIT",
  "dependencies": {
    "css-query-selector": "*",
    "marked": "*",
    "organism-react-animate": "*",
    "organism-react-apple-device": "*",
    "organism-react-d3-piechart": "*",
    "organism-react-event-timeline": "*",
    "organism-react-geometryangle": "*",
    "organism-react-html2canvas": "*",
    "organism-react-navigation": "*",
    "organism-react-popup": "*",
    "organism-react-scroll-animate": "*",
    "organism-react-scroll-nav": "*",
    "organism-react-tag-highlight": "*",
    "organism-react-typing": "*",
    "pmvc_react_landing": "*",
    "react-atomic-molecule": "*",
    "react-atomic-organism": "*",
    "ricon": "*"
  },
  "devDependencies": {
    "canvas": "*",
    "react": "^18.x",
    "react-dom": "^18.x",
    "reshow-app": "*"
  },
  "exports": {
    "require": "./build/cjs/src/index.js",
    "import": "./build/es/src/index.mjs"
  },
  "main": "./build/cjs/src/index.js",
  "module": "./build/es/src/index.mjs",
  "scripts": {
    "update-compile-sh": "yo reshow:compile-sh",
    "webpack": "webpack",
    "start": "ws",
    "format": "prettier-eslint --write 'src/**/*.js' 'ui/**/*.js*'",
    "clean:webpack": "find ./assets -name '*.*' | xargs rm -rf && rm -rf workbox-*.js",
    "clean": "find ./build -name '*.*' | xargs rm -rf",
    "build:cjs:src": "BABEL_ENV=cjs babel src -d build/cjs/src --ignore /**/__tests__",
    "build:cjs:ui": "BABEL_ENV=cjs babel ui -d build/cjs/ui --ignore /**/__tests__",
    "build:cjs": "npm run build:cjs:src && npm run build:cjs:ui",
    "build:es:src": "BABEL_ENV=es babel src -d build/es/src --out-file-extension .mjs",
    "build:es:ui": "BABEL_ENV=es babel ui -d build/es/ui --out-file-extension .mjs",
    "build:es": "npm run build:es:src && npm run build:es:ui",
    "build": "npm run clean && npm run build:cjs && npm run build:es",
    "mochaFor": "STRICT_MODE=on mocha -r global-jsdom/register",
    "mocha": "npm run mochaFor -- 'build/es/**/__tests__/*.mjs'",
    "test": "npm run build && npm run mocha",
    "prepublishOnly": "npm run test"
  },
  "files": [
    "build",
    "photos",
    "package.json",
    "README.md"
  ]
}
